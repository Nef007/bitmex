    "config": "^3.3.6",
    "express": "^4.17.1",
    "express-validator": "^6.10.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mongoose": "^6.0.4",
    "mysql2": "^2.3.0",
    "node-fetch": "^2.6.2",
    "qs": "^6.10.1",
    "request": "^2.88.2",
    "sequelize": "^6.6.5",
    "shortid": "^2.2.16"
  },
  "devDependencies": {
    "concurrently": "^6.0.0",
